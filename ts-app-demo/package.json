{
  "name": "ts-app-demo",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start": "concurrently npm:start-app npm:start-api",
    "start-app": "cross-env VITE_APP_API_BASE_URL=http://localhost:3001/ vite --port 3000 --open --strictPort",
    "start-api": "json-server --port 3001 --watch db.json --delay 0",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview"
  },
  "dependencies": {
    "@faker-js/faker": "^8.4.1",
    "@tanstack/react-query": "^5.34.2",
    "@tanstack/react-query-devtools": "^5.34.2",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "jotai": "^2.8.0",
    "json-server": "0.17.4",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-error-boundary": "^4.0.13",
    "react-hot-toast": "^2.4.1",
    "react-router-dom": "^6.23.0",
    "recoil": "^0.7.7",
    "valtio": "^1.13.2",
    "xstate": "^5.13.0",
    "zustand": "^4.5.2"
  },
  "devDependencies": {
    "@types/react": "^18.3.1",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^7.8.0",
    "@typescript-eslint/parser": "^7.8.0",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.6",
    "typescript": "^5.4.5",
    "vite": "^5.2.11"
  }
}
